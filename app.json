{
  "expo": {
    "name": "training-tracker",
    "slug": "training-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "trainingtracker",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#06B6D4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mardanov.training-tracker"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "android": {
      "package": "com.mardanov.trainingtracker"
    }
  }
}
